@model IEnumerable<HoneyAllViewModel>

@foreach (var item in Model)
{
    <a asp-area="" asp-controller="Honey" asp-action="Details" asp-route-id="@item.Id">
        <div class="single_food_item media">
            <img src="@item.ImageUrl" 
                 class="mr-3" 
                 alt="@item.Title" 
                 style="width: 150px; height: 150px; object-fit: cover;"
                 onerror="if(this.src.indexOf('data:') !== 0) { this.src='data:image/svg+xml;base64,'; this.alt='Мед'; this.onerror=null; }">
            <div class="media-body align-self-center">
                <h3>@item.Title</h3>
                <p>@item.Origin</p>
                <h5>@item.Price лв</h5>
            </div>
        </div>
    </a>
}